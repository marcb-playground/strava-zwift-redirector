[run]
branch = True
source = strava_zwift_redirector
omit =
    tests/*
    */__init__.py
    */__main__.py

[report]
show_missing = True
precision = 2
exclude_lines =
    pragma: no cover
    def __repr__
    if __name__ == .__main__.:  # pragma: no cover
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:  # pragma: no cover
    if TYPE_CHECKING:

[html]
directory = htmlcov
