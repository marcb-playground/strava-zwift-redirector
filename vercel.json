{
  "version": 2,
  "builds": [
    {
      "src": "strava-zwift-redirector/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/strava-notification",
      "dest": "strava-zwift-redirector/index.py"
    },
    {
      "src": "(.*)",
      "dest": "strava-zwift-redirector/index.py"
    }
  ]
}